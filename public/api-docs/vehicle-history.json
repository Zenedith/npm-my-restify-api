{
  "apiVersion": "1.0.0",
  "swaggerVersion": "1.2",
  "basePath1": "http://localhost:3000",
  "basePath": "http://vehicle-history-api.herokuapp.com",
  "resourcePath": "/api/vehicle-history",
  "apis": [
    {
      "path": "/api/vehicle-history",

      "operations": [
        {
          "produces": [
            "application/json; charset=utf-8"
          ],
          "consumes": [
            "application/x-www-form-urlencoded"
          ],
          "method": "GET",
          "summary": "Find vehicle history",
          "notes": "Returns a vehicle info and history",
          "type": "Response",
          "nickname": "postVehicleHistory",
          "authorizations": {},
          "parameters": [
            {
              "name": "plate",
              "description": "Car plate number",
              "required": true,
              "type": "string",
              "defaultValue": "ABC1234",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "vin",
              "description": "Car vin number",
              "required": true,
              "type": "string",
              "defaultValue": "ABC123456789DEF",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "firstRegistrationDate",
              "description": "Car first registration date",
              "required": true,
              "type": "string",
              "defaultValue": "11.12.2014",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "captcha",
              "description": "Service captcha",
              "required": false,
              "type": "string",
              "paramType": "query",
              "allowMultiple": false
            }
          ],
          "responseMessages": [
            {
              "code": 404,
              "message": "Unable to find vehicle history"
            }
          ]
        }
      ]
    }
  ],
  "models": {
    "Response": {
      "id": "Response",
      "properties": {
        "car": {
          "$ref": "Car"
        },
        "events": {
          "type": "Array",
          "items": {
            "$ref": "Event"
          }
        }
      }
    },
    "Car": {
      "id": "Car",
      "properties": {
        "name": {
          "$ref": "Name",
          "required": false,
          "description": "car name"
        },
        "type": {
          "$ref": "Type",
          "required": false,
          "description": "car type"
        },
        "engine": {
          "$ref": "Engine",
          "required": false,
          "description": "car engine"
        },
        "production": {
          "$ref": "Production",
          "description": "car production",
          "required": false
        },
        "policy": {
          "$ref": "Policy",
          "required": false,
          "description": "car policy"
        },
        "registration": {
          "$ref": "Registration",
          "required": false,
          "description": "car registration"
        },
        "inspection": {
          "$ref": "Inspection",
          "required": false,
          "description": "car inspection"
        },
        "mileage": {
          "$ref": "Mileage",
          "description": "Mileage can be null value",
          "required": false
        },
        "stolen": {
          "type": "boolean",
          "description": "is car stolen",
          "required": false
        },
        "plate": {
          "$ref": "Plate",
          "description": "car plate",
          "required": false
        },
        "vin": {
          "type": "string",
          "description": "car vin number",
          "required": false
        }
      }
    },
    "Name": {
      "id": "Name",
      "properties": {
        "make": {
          "type": "string",
          "description": "car make",
          "required": true,
          "enum": [
            "AUDI",
            "BMW",
            "CHEVROLET",
            "CHRYSLER",
            "CITROEN",
            "DACIA",
            "DAEWOO",
            "FIAT",
            "FSO",
            "FORD",
            "HONDA",
            "HYUNDAI",
            "JAGUAR",
            "KIA",
            "LEXUS",
            "MERCEDES",
            "MAZDA",
            "MINI",
            "MITSUBISHI",
            "NISSAN",
            "OPEL",
            "PEUGEOT",
            "RENAULT",
            "SEAT",
            "SKODA",
            "SUZUKI",
            "TOYOTA",
            "VOLKSWAGEN",
            "VOLVO",
            "UNKNOWN"
          ]
        },
        "name": {
          "type": "string",
          "description": "car name",
          "required": false
        },
        "model": {
          "type": "string",
          "description": "car model",
          "required": true
        }
      }
    },
    "Production": {
      "id": "Production",
      "properties": {
        "year": {
          "type": "string",
          "description": "production year",
          "required": true
        }
      }
    },
    "Plate": {
      "id": "Plate",
      "properties": {
        "value": {
          "type": "string",
          "description": "plate value",
          "required": true
        },
        "country": {
          "type": "string",
          "description": "plate country",
          "required": true,
          "enum": [
            "PL",
            "UK",
            "UNKNOWN"
          ]
        }
      }
    },
    "Type": {
      "id": "Type",
      "properties": {
        "type": {
          "type": "string",
          "description": "car type",
          "required": true,
          "enum": [
            "CAR",
            "VAN"
          ]
        },
        "kind": {
          "type": "string",
          "description": "car kind",
          "required": true,
          "enum": [
            "HATCHBACK",
            "LIMOUSINE",
            "SEDAN",
            "SEDAN",
            "COUPE",
            "COMBI",
            "OTHER"
          ]
        }
      }
    },
    "Engine": {
      "id": "Engine",
      "properties": {
        "cubicCapacity": {
          "type": "string",
          "description": "car cubic capacity (cmÂ³)",
          "required": false
        },
        "fuel": {
          "type": "string",
          "description": "car engine fuel type",
          "required": true,
          "enum": [
            "PETROL",
            "CNG",
            "LPG",
            "DIESEL",
            "ETANOL",
            "HYBRID",
            "ELECTRIC",
            "HYDROGEN",
            "OTHER"
          ]
        }
      }
    },
    "Policy": {
      "id": "Policy",
      "properties": {
        "status": {
          "type": "string",
          "description": "car policy status",
          "required": true,
          "enum": [
            "UNKNOWN",
            "UPTODATE",
            "OUTDATED"
          ]
        }
      }
    },
    "Inspection": {
      "id": "Inspection",
      "properties": {
        "status": {
          "type": "string",
          "description": "car inspection status",
          "required": true,
          "enum": [
            "UPTODATE",
            "OUTDATED",
            "UNKNOWN"
          ]
        }
      }
    },
    "Registration": {
      "id": "Registration",
      "properties": {
        "status": {
          "type": "string",
          "description": "registration status",
          "required": true,
          "enum": [
            "UNKNOWN",
            "REGISTERED",
            "UNREGISTERED"
          ]
        },
        "firstAt": {
          "type": "string",
          "description": "registration first at date",
          "required": false
        }
      }
    },
    "Mileage": {
      "id": "Mileage",
      "properties": {
        "value": {
          "type": "string",
          "description": "car mileage value",
          "required": false
        },
        "type": {
          "type": "string",
          "description": "car mileage type",
          "required": true,
          "enum": [
            "KM",
            "MILE"
          ]
        }
      }
    },
    "Event": {
      "id": "Event",
      "properties": {
        "type": {
          "type": "string",
          "description": "car mileage type",
          "required": true,
          "enum": [
            "PRODUCTION",
            "REGISTRATION",
            "DEREGISTRATION",
            "CHANGE_OWNER",
            "INSPECTON",
            "UNKNOWN"
          ]
        },
        "createdAt": {
          "type": "string",
          "description": "event created at date",
          "required": true
        },
        "expireAt": {
          "type": "string",
          "description": "event expire at date, used for INSPECTION event type",
          "required": false
        },
        "note": {
          "type": "string",
          "description": "event note, used for DEREGISTRATION event type",
          "required": false
        },
        "firstOwner": {
          "type": "string",
          "description": "event first owner (true/false) information, used for CHANGE_OWNER event type",
          "required": false
        },
        "ownerType": {
          "type": "string",
          "description": "event owner type, used for CHANGE_OWNER event type",
          "required": false,
          "enum": [
            "PRIVATE"
          ]
        },
        "location": {
          "$ref": "Location",
          "description": "event location, used for CHANGE_OWNER event type",
          "required": false
        },
        "abroadRegistration": {
          "type": "string",
          "description": "event abroad registration (true/false) information, used for REGISTRATION event type",
          "required": false
        }
      }
    },
    "Location": {
      "id": "Location",
      "properties": {
        "country": {
          "type": "string",
          "description": "location country",
          "required": true,
          "enum": [
            "PL",
            "UK",
            "UNKNOWN"
          ]
        },
        "state": {
          "type": "string",
          "description": "location state",
          "required": false
        }
      }
    }
  }
}
